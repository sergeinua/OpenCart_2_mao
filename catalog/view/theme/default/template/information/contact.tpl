		var close2 = $('#close-2');
		var close3 = $('#close-3');

      marker1.setMap(map);
      marker2.setMap(map);
      marker3.setMap(map);

		function toggleBounce(mrker) {
			if (mrker.getAnimation() !== null) {
				mrker.setAnimation(null);
			} else {
				mrker.setAnimation(google.maps.Animation.BOUNCE);
			}
		}

		function delBounce(mrker) {
			if (mrker.getAnimation() !== null) {
				mrker.setAnimation(null);
			}
		}

		marker1.addListener('click', function() {
			toggleBounce(marker1);
			delBounce(marker2);
			delBounce(marker3);
			$('#info-block-1').addClass('open');
			$('#info-block-2').removeClass('open');
			$('#info-block-3').removeClass('open');
		});

		marker2.addListener('click', function() {
			toggleBounce(marker2);
			delBounce(marker1);
			delBounce(marker3);
			$('#info-block-2').addClass('open');
			$('#info-block-1').removeClass('open');
			$('#info-block-3').removeClass('open');
		});

		marker3.addListener('click', function() {
			toggleBounce(marker3);
			delBounce(marker2);
			delBounce(marker1);
			$('#info-block-3').addClass('open');
			$('#info-block-2').removeClass('open');
			$('#info-block-1').removeClass('open');
		});

		close1.click(function(){
			toggleBounce(marker1);
			$('#info-block-1').removeClass('open');
		});
		close2.click(function(){
			toggleBounce(marker2);
			$('#info-block-2').removeClass('open');
		});
		close3.click(function(){
			toggleBounce(marker3);
